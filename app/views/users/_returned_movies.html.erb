<% if @movie_array.present? %>
  <% @movie_array.each do |movie| %>
  <div class="movie-display">
    <div class="movie-case">
      <div id="movie-title"><%= movie.title %></div>
      <img src=<%=movie.poster_url%> alt="Movie Poster" class="movie-poster">
      </div>
    </div>
  <% end %>
<% end %>